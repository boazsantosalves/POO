class Main{
  public static void main(String args[]){
    int x[] = new int[4];
    x[0]=1;
    x[1]=2;

    int j;
    for(j=0; j<2; j++){ /*vai até a posição menor que 2 ( x[1] ), que foi até onde eu coloquei números no vetor*/
      
      System.out.println(x[j] + "\n");


    }

  
    int i;
    for(i=0; i<=5; i++){
      System.out.println(i);
    }
  }
}
